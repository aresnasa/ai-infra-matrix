{
    // VS Code NoProxy 配置模板
    // 将以下内容添加到你的 VS Code 用户设置中
    // 路径: ~/Library/Application Support/Code/User/settings.json
    
    "http.proxySupport": "on",
    "http.noProxy": [
        // 本地回环地址
        "localhost",
        "127.0.0.1",
        "::1",
        
        // 本地域名
        "*.local",
        "*.internal",
        "*.intranet",
        
        // 私有网络地址段
        "10.*",
        "192.168.*",
        
        // Docker 内部网络
        "172.16.*",
        "172.17.*",
        "172.18.*",
        "172.19.*",
        "172.20.*",
        "172.21.*",
        "172.22.*",
        "172.23.*",
        "172.24.*",
        "172.25.*",
        "172.26.*",
        "172.27.*",
        "172.28.*",
        "172.29.*",
        "172.30.*",
        "172.31.*",
        
        // Kubernetes 集群网络
        "*.cluster.local",
        "*.svc.cluster.local",
        
        // 常见的内网域名
        "*.corp",
        "*.company",
        "*.dev",
        "*.test",
        "*.staging",
        
        // 如果你有特定的域名或IP，请在这里添加
        // "your-internal-domain.com",
        // "your-ip-address"
    ]
}