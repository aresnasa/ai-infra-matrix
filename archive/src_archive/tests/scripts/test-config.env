# 端到端测试配置文件
# 测试环境配置

# 服务地址配置
BASE_URL="http://localhost:8082/api"
FRONTEND_URL="http://localhost:3001"

# 测试用户凭证
ADMIN_USERNAME="admin"
ADMIN_PASSWORD="admin123"

# 测试项目配置
TEST_PROJECT_NAME="e2e-test-project"
TEST_PROJECT_DESCRIPTION="端到端测试项目"

# 测试用户配置
TEST_USER_USERNAME="e2e_testuser"
TEST_USER_EMAIL="e2etest@example.com"
TEST_USER_PASSWORD="e2etest123"

# 测试主机配置
TEST_HOST_NAME="test-host"
TEST_HOST_IP="192.168.1.100"
TEST_HOST_USER="root"
TEST_HOST_PORT="22"

# 测试变量配置
TEST_VAR_NAME="test_var"
TEST_VAR_VALUE="test_value"
TEST_VAR_TYPE="string"

# 测试任务配置
TEST_TASK_NAME="test-task"
TEST_TASK_MODULE="debug"
TEST_TASK_ARGS="msg=Hello World"

# 容器时区配置
EXPECTED_TIMEZONE="CST"
BACKEND_CONTAINER="ansible-backend"
FRONTEND_CONTAINER="ansible-frontend"

# 测试超时配置
SERVICE_STARTUP_TIMEOUT=30
API_REQUEST_TIMEOUT=10

# 测试开关配置
ENABLE_USER_MANAGEMENT_TESTS=true
ENABLE_HEALTH_CHECK_TESTS=true
ENABLE_TRASH_FUNCTIONALITY_TESTS=true
ENABLE_TIMEZONE_TESTS=true
ENABLE_DOWNLOAD_TESTS=true

# 调试配置
DEBUG_MODE=false
VERBOSE_OUTPUT=false
KEEP_TEST_DATA=false
